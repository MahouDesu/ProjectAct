@model ActInteraction.ViewModels.ViewModel
@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>
<body>
    <table class="table table-bordered table-hover">
        <thead>
            <td>
                Name
            </td>
            <td>
                DPS
            </td>
            <td>
                Duration
            </td>
        </thead>
        @{
            for (int i = 0; i < Model.DamageChart.Count; i++)
            {
                <tr>
                    <td>
                        @Model.DamageChart[i].name
                    </td>
                    <td>
                        @Model.DamageChart[i].dps
                    </td>
                    <td>
                        @Model.DamageChart[i].duration
                    </td>
                </tr>
            }
        }
    </table>
</body>

